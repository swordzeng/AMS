    "import sqlite3\n",
    "import pandas as pd\n",
    "\n",
    "db = sqlite3.connect('AMS.db')\n",
    "query = 'select * from Order_Table'\n",
    "df = pd.read_sql(query, con = db)\n",
    "df = df[df['TradeID']==1]\n",
    "df.groupby(['Date','OrderID','TradeID'])[['SettleAmt']].sum()"
   ]
